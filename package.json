{
  "name": "harvest",
  "version": "1.0.0",
  "description": "harvest",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "client:start": "parcel client/index.html",
    "server:start": "nodemon --watch server -e ts --exec npx ts-node server/index.ts",
    "db:init": "ts-node server/dbTools/init",
    "db:make": "ts-node server/dbTools/make",
    "db:start": "npm-run-all db:init db:make",
    "db:start-rebuild": "REBUILD=true npm-run-all db:init db:make"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "@svgr/parcel-plugin-svgr": "^5.1.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.4",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "pg": "^8.0.0",
    "pg-promise": "^10.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^6.8.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.1",
    "jest": "^25.3.0",
    "less": "^3.11.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.1",
    "typescript": "^3.7.5"
  },
  "resolutions": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/dotenv": "^8.2.0"
  }
}
